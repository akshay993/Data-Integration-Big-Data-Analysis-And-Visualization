r .w-tomorrow a{vertical-align:baseline}.nyt-weather .w-tomorrow .w-icon{height:20px;margin-left:5px;vertical-align:middle}.nyt-weather .w-tomorrow .w-high,.nyt-weather .w-tomorrow .w-low{font-weight:400;margin:0;font-size:14px;-webkit-transform:none;transform:none}.nyt-weather .w-tomorrow .w-link{margin-left:10px;float:right;text-transform:none}.viewport-medium .nyt-weather .w-tomorrow{display:inline-block;width:60%;text-align:right;border-top:none;padding-top:0;margin-top:0;position:absolute;bottom:0;right:0}.viewport-medium .nyt-weather .w-tomorrow .w-high,.viewport-medium .nyt-weather .w-tomorrow .w-low{font-size:15px}

